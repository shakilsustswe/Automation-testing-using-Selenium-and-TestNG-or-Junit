    "version": "0.2.0",
    "configurations": [
        {
            "name": "Java",
            "type": "java",
            "request": "launch",
            "stopOnEntry": true,
            "jdkPath": "${env:JAVA_HOME}/bin",
            "cwd": "${fileDirname}",
            "startupClass": "${fileBasenameNoExtension}",
            "classpath": [
                ".",
                "${fileDirname}"
            ]
        },
        {
            "name": "Java Console App",
            "type": "java",
            "request": "launch",
            "stopOnEntry": true,
            "jdkPath": "${env:JAVA_HOME}/bin",
            "cwd": "${fileDirname}",
            "startupClass": "${fileBasenameNoExtension}",
            "classpath": [
                ".",
                "${fileDirname}"
            ],
            "externalConsole": true
        },
        {
            "type": "java",
            "name": "Launch Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Launch CssXpath",
            "request": "launch",
            "mainClass": "test.CssXpath",
            "projectName": "test"
        },
        {
            "type": "java",
            "name": "Launch LogingTest",
            "request": "launch",
            "mainClass": "test.LogingTest",
            "projectName": "test"
        },
        {
            "type": "java",
            "name": "Launch QuestionNumber1",
            "request": "launch",
            "mainClass": "test.QuestionNumber1",
            "projectName": "test"
        },
        {
            "type": "java",
            "name": "Launch Shakil",
            "request": "launch",
            "mainClass": "test.Shakil",
            "projectName": "test"
        },
        {
            "type": "java",
            "name": "Launch TestClass",
